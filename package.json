{
    "name": "wen-vant-preview",
    "version": "0.1.5",
    "description": "基于vant2.x的swipe组件改造，支持视频和图片预览的vant组件（适用移动端）",
    "scripts": {
        "i-all": "npm i & cd example && npm i",
        "start": "start npm run watch & start npm run example",
        "dev": "webpack --config build/webpack.config.dev.js",
        "watch": "webpack --config build/webpack.config.dev.js --watch",
        "example": "cd example && npm run serve",
        "dist": "webpack --config build/webpack.config.prod.js",
        "release": "npm run dist & npm publish"
    },
    "author": "cxw18679850773",
    "license": "MIT",
    "devDependencies": {
        "babel-plugin-import": "^1.13.5",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.3",
        "html-webpack-plugin": "^5.5.0",
        "vue-loader": "^15.9.2",
        "vue-mini-player": "^0.2.1",
        "vue-template-compiler": "^2.7.5",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    },
    "main": "./dist/",
    "dependencies": {
        "vue-mini-player": "^0.2.1"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/18679850773/wen-vant-preview.git"
    }
}
